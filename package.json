{
  "name": "@slidev/theme-daocloud",
  "version": "1.1.0",
  "description": "DaoCloud themed Slidev presentation",
  "main": "index.ts",
  "keywords": [
    "slidev-theme",
    "slidev",
    "daocloud",
    "presentation"
  ],
  "author": "DaoCloud",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "slidev": ">=0.48.0"
  },
  "scripts": {
    "build": "slidev build",
    "dev": "slidev --open",
    "export": "slidev export"
  },
  "dependencies": {
    "@slidev/cli": "*",
    "@slidev/types": "^0.49.0"
  },
  "devDependencies": {
    "@unocss/reset": "^0.58.0",
    "slidev-component-progress": "^1.1.0",
    "unocss": "^0.58.0"
  },
  "files": [
    "layouts",
    "components",
    "styles",
    "setup",
    "index.ts"
  ],
  "peerDependencies": {
    "@slidev/cli": "*"
  },
  "slidev": {
    "colorSchema": "auto",
    "highlighter": "shiki",
    "lineNumbers": false,
    "monaco": "dev",
    "fonts": {
      "sans": "Robot",
      "serif": "Robot Slab",
      "mono": "Fira Code"
    },
    "addons": [
      "slidev-component-progress"
    ]
  }
}
